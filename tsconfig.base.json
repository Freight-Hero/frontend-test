{
  "compilerOptions": {
    "target": "ES2023",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "sourceMap": true,
    "composite": true,
    "incremental": true,
    "strict": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules"
  ]
}